{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "Solana Mobile Docs",
  "colors": {
    "primary": "#000000",
    "light": "#EF5C6F",
    "dark": "#EF5C6F"
  },
  "background": {
    "decoration": "gradient"
  },
  "appearance": {
    "default": "dark"
  },
  "metadata": {
    "timestamp": true
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude"]
  },
  "fonts": {
